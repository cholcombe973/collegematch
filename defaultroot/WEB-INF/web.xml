<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
  <servlet>
    <servlet-name>picservlet</servlet-name>
    <servlet-class>collegematch.PicServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>paypal</servlet-name>
    <servlet-class>collegematch.Paypal</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>pdt</servlet-name>
    <servlet-class>collegematch.PDT</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>accountvalidate</servlet-name>
    <servlet-class>collegematch.AccountValidate</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>picservlet</servlet-name>
    <url-pattern>/picservlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>paypal</servlet-name>
    <url-pattern>/paypal</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>pdt</servlet-name>
    <url-pattern>/pdt</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>accountvalidate</servlet-name>
    <url-pattern>/accountvalidate</url-pattern>
  </servlet-mapping>
  <resource-ref>
    <description>CollegeMatch connection pool</description>
    <res-ref-name>jdbc/CollegeMatch</res-ref-name>
    <res-type>javax.sql.DataSource</res-type>
    <res-auth>Container</res-auth>
  </resource-ref>
</web-app>
